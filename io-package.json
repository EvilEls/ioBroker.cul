        "title": "CUL/culfw",
        "desc": "Javascript/Node.js based Busware CUL USB / culfw adapter",
        "platform": "javascript/Node.js",
        "mode": "daemon",
        "loglevel": "info",
        "license": "GPL-2.0",
        "enabled": true,
        "messagebox": true,
        "icon": "busware.jpg",
        "extIcon": "https://raw.githubusercontent.com/ioBroker/ioBroker.cul/master/admin/busware.jpg",
